<%= simple_form_for(contact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :phone_number %>
    <%= f.input :address %>
    <%= f.input :property_id, collection: Property.all %>
  </div>

  <div class="form-actions">
    <%= f.button :button, class: 'btn-primary'  %>
  </div>
  </br>
<% end %>
